   "source": [
    "def alphabeta(e1x, e2x, e1y, e2y, x0, y0, x, y):\n",
    "    C = np.array([[x-x0],[y-y0]])\n",
    "    B = np.array([[e1x,e2x],[e1y,e2y]])\n",
    "    B = np.linalg.inv(B)\n",
    "    A = np.dot(B,C)\n",
    "    return A"
   ]
