    "def sdpoints(e1x,e1y,e2x,e2y,x0,y0,m):\n",
    "    alpha=m[0][0]\n",
    "    beta =m[1][0]\n",
    "    a=x0+(alpha*e1x)+(beta*e2x)\n",
    "    b=y0+(alpha*e1y)+(beta*e2y)\n",
    "    a=round(a)\n",
    "    b=round(b)\n",
    "    a=int(a)\n",
    "    b=int(b)\n",
    "    return a,b"
   ]
